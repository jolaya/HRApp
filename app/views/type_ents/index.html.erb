<p id="notice"><%= notice %></p>

<h1>Listing Type Ents</h1>

<table>
  <thead>
    <tr>
      <th>Initial</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_ents.each do |type_ent| %>
      <tr>
        <td><%= type_ent.initial %></td>
        <td><%= type_ent.description %></td>
        <td><%= link_to 'Show', type_ent %></td>
        <td><%= link_to 'Edit', edit_type_ent_path(type_ent) %></td>
        <td><%= link_to 'Destroy', type_ent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

