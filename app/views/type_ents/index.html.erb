<p id="notice"><%= notice %></p>

<h1>Listing Type Ents</h1>

<div class="media">
  <div class="media-body">
  	<table class="table table-hover">
      <thead>
        <tr>
          <th>Initial</th>
          <th>Description</th>
          <th colspan="3"></th>
        </tr>
      </thead>
    
      <tbody>
        <% @type_ents.each do |type_ent| %>
          <tr>
            <td><%= type_ent.initial %></td>
            <td><%= type_ent.description %></td>
            <td><%= link_to 'Show', type_ent, class: 'btn btn-mini' %></td>
            <td><%= link_to 'Edit', edit_type_ent_path(type_ent), class: 'btn btn-mini btn-success' %></td>
            <td><%= link_to 'Destroy', type_ent, method: :delete, class: 'btn btn-mini btn-danger', data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<br>

<%= link_to 'New Type ent', new_type_ent_path %>
