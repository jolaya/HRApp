<p id="notice"><%= notice %></p>

<p>
  <strong>Staff:</strong>
  <%= @profile.staff.staff_number + ' - ' + @profile.staff.name + ',' + @profile.staff.surname %>
</p>

<p>
  <strong>User:</strong>
  <%= @profile.user.email %>
</p>

<%= link_to 'Edit', edit_profile_path(@profile) %> |

<% if current_user.admin? %>
    <%= link_to 'Back', profiles_path %>
<% else %>
    <%= link_to 'Back', root_path %>
<% end %>
